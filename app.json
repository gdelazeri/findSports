{
  "expo": {
    "name": "Find Sports",
    "slug": "findsports",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "androidStatusBar": {
      "backgroundColor": "#808080"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#ffffff",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.findsports"
    },
    "android": {
      "versionCode": 1,
      "package": "com.findsports"
    }
  }
}
